@model List<ITManagement.Models.DeviceHistory>

@{
    ViewData["Title"] = "Device History";
}

<h2>Device History</h2>

<table class="table">
    <thead>
        <tr>
            <th>Device ID</th>
            <th>Device Name</th>
            <th>Category ID</th>
            <th>Category Name</th>
            <th>User ID</th>
            <th>User Name</th>
            <th>Updated By</th>
            <th>Updated At</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.DeviceId</td>
                <td>@item.DeviceName</td>
                <td>@item.CategoryId</td>
                <td>@item.CategoryName</td>
                <td>@item.UserId</td>
                <td>@item.UserName</td>
                <td>@item.UpdatedBy</td>
                <td>@item.UpdatedAt</td>
                <td>@item.Description</td>
            </tr>
        }
    </tbody>
</table>
